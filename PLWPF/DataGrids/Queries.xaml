<Window x:Class="PLWPF.DataGrids.Queries"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF.DataGrids"
        mc:Ignorable="d"
        Title="Queries" Height="450" Width="800">
    <Grid>
        <Button x:Name="GRArea" Content="הצג יחידות אירוח לפי איזור" HorizontalAlignment="Left" Margin="229,128,0,0" VerticalAlignment="Top" Width="314" Click="GRArea_Click" RenderTransformOrigin="0.5,0.5" Height="52">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btnGRType" Content="הצג יחידות אירוח לפי סוג אירוח" HorizontalAlignment="Left" Margin="225,206,0,0" VerticalAlignment="Top" Width="318" Height="51" Click="btnGRType_Click"/>
    </Grid>
</Window>
